{
    "version": 2,
    "builds": [
      { "src": "billingApp/wsgi.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },  
      { "src": "/media/(.*)", "dest": "/media/$1" },    
      { "src": "/(.*)", "dest": "billingApp/wsgi.py" }  
    ]
  }