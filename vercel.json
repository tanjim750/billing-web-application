{
    "builds": [{
        "src": "billingApp/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "billingApp/wsgi.py"
        },
        { 
            "src": "/static/(.*)", 
            "dest": "/static/$1" 
        },
        { 
            "src": "/media/(.*)", 
            "dest": "/media/$1" 
        }
    ]
}
